{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, s-maxage=600, must-revalidate"
                }
            ]
        },
        {
            "source": "/(.*)\\.(css|js|jpg|jpeg|png|gif|webp|woff2|svg)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}
